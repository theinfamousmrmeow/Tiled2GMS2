//arg0 = character
switch(argument0){
	case DeathKnight:
		return Morningstar;
	break;
	
	case Valkyrie:
		return Hammer;
	break;
	
	case Goblin:
		return Beetlerang;
	break;
	
	case Troll:
		return Boulder;
	break;
	
	case Reaper:
		return Scythe;
	break;
	
	case Wizard:
		return Fireball;
	break;
	
	default:
		return Axe;
	break;
}