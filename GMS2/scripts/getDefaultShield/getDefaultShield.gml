//arg0 = character
switch(argument0){
	case DeathKnight:
		return SkullShield;
	break;
	
	case Valkyrie:
		return GorgonShield;
	break;
	
	case Goblin:
		return CarapaceShield;
	break;
	
	case Troll:
		return WoodShield;
	break;
	
	case Reaper:
		return SkullShield;
	break;
	
	default:
		return BlueShield;
	break;
}